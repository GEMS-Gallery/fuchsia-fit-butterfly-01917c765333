type Result_1 = 
 variant {
   err: text;
   ok: nat;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
type GroceryItem = 
 record {
   completed: bool;
   emoji: text;
   id: nat;
   name: text;
 };
service : {
  addItem: (text, text) -> (Result_1);
  getItems: () -> (vec GroceryItem) query;
  toggleItemCompletion: (nat) -> (Result);
}
